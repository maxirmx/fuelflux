    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug fuelflux (CMake Build)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/bin/fuelflux.exe",
            "args": [],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "preLaunchTask": "Build fuelflux",
            "MIMode": "gdb",
            "miDebuggerPath": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
